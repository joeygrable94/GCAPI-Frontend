{
  "name": "example-with-auth",
  "type": "module",
  "scripts": {
    "generate-client": "openapi -i http://localhost:3333/api/docs/openapi.json -o ./src/shared/api -c axios --useOptions --exportSchemas true",
    "generate-openapi-client": "npx openapi-typescript http://localhost:3333/api/docs/openapi.json --path-params-as-types --array-length --additional-properties --enum --enum-types -o ./src/shared/api/openapi.schema.d.ts",
    "generate-openapi-ts": "openapi-ts --input http://localhost:3333/api/docs/openapi.json --output ./src/shared/api --useOptions --client axios",
    "openapi-ts": "openapi-ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint --config .eslintrc.js 'src/**/*.ts' --fix",
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "version": "vinxi version",
    "run": "node -r dotenv/config .output/server/index.mjs"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.46.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-solid": "^0.14.0",
    "next-auth": "^4.24.7",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "@auth/core": "^0.29.0",
    "@babel/core": "7.24.4",
    "@hey-api/client-fetch": "^0.1.3",
    "@kobalte/core": "^0.13.1",
    "@modular-forms/solid": "^0.20.0",
    "@solid-mediakit/auth": "^2.0.11",
    "@solid-mediakit/auth-plugin": "^1.0.4",
    "@solid-primitives/storage": "^3.5.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.13.3",
    "@solidjs/start": "^1.0.0",
    "@tanstack/solid-form": "^0.20.0",
    "@tanstack/solid-query": "^5.37.1",
    "@tanstack/solid-query-devtools": "^5.37.1",
    "@tanstack/solid-table": "^8.17.3",
    "@tanstack/zod-form-adapter": "^0.20.0",
    "axios": "^1.7.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "solid-bootstrap": "^1.0.20",
    "solid-heroicons": "^3.2.4",
    "solid-js": "^1.8.17",
    "solid-toast": "^0.5.0",
    "valibot": "^0.30.0",
    "vinxi": "^0.3.11",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18"
  }
}
