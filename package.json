{
  "name": "gcapi-solid-start",
  "scripts": {
    "generate-client": "openapi --input http://localhost:8888/api/v1/docs/openapi.json --output ./src/api --useOptions --useUnionTypes --exportSchemas true --indent 2",
    "generate-client-docker": "openapi --input http://0.0.0.0:8888/api/v1/docs/openapi.json --output ./src/api --useOptions --useUnionTypes --exportSchemas true --indent 2",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "dev": "solid-start dev",
    "build": "npm run format && solid-start build",
    "start": "solid-start start"
  },
  "type": "module",
  "devDependencies": {
    "@suid/vite-plugin": "^0.1.4",
    "@types/auth0-js": "^9.14.7",
    "@types/node": "^18.11.18",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "sass": "^1.57.1",
    "solid-start-node": "^0.2.19",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-solid": "^2.7.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.2",
    "@suid/icons-material": "^0.6.3",
    "@suid/material": "^0.12.3",
    "auth0-js": "^9.21.0",
    "axios": "^1.4.0",
    "solid-js": "^1.7.2",
    "solid-start": "^0.2.26",
    "undici": "^5.15.1"
  },
  "engines": {
    "node": ">=16.8"
  }
}
