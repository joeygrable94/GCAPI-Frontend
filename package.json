{
  "name": "app",
  "scripts": {
    "generate-client": "openapi --input http://localhost:8888/api/v1/docs/openapi.json --output ./src/api --useOptions --useUnionTypes --exportSchemas true --indent 2",
    "generate-client-docker": "openapi --input http://backend:8888/api/v1/docs/openapi.json --output ./src/api --useOptions --useUnionTypes --exportSchemas true --indent 2",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "dev": "solid-start dev",
    "build": "npm run format && solid-start build",
    "start": "solid-start start"
  },
  "type": "module",
  "devDependencies": {
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "sass": "^1.57.1",
    "solid-start-node": "^0.2.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@hope-ui/core": "^1.0.0-next.11",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.6.0",
    "@stitches/core": "^1.2.8",
    "@tanstack/solid-table": "^8.7.6",
    "clsx": "^1.2.1",
    "solid-icons": "^1.0.4",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.0",
    "undici": "^5.11.0"
  },
  "engines": {
    "node": ">=16.8"
  }
}
