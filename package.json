    "@types/node": "^18.11.18",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vite-plugin-solid": "^2.7.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
